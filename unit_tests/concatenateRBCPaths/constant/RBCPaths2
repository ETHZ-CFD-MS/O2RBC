FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RBCPath;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RBCPath111
{
    index     111;
    times     3(3 4 5);
    edges     3(1 2 3);
    sCoords   3(0.1 0.3 0.7);
}

RBCPath333
{
    index     333;
    times     3(3 4 5);
    edges     3(1 2 3);
    sCoords   3(0.1 0.3 0.7);
}
