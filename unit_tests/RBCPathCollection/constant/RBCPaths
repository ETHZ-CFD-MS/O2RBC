FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RBCPath;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

RBCPath1
{
    index     1;
    times     3(0 1 2);
    edges     3(1 2 3);
    sCoords   3(0.1 0.3 0.7);
}

RBCPath2
{
    index     2;
    times     3(1 2 3);
    edges     3(1 2 3);
    sCoords   3(0.1 0.3 0.7);
}

RBCPath3
{
    index     3;
    times     3(2 3 4);
    edges     3(1 2 3);
    sCoords   3(0.1 0.3 0.7);
}
