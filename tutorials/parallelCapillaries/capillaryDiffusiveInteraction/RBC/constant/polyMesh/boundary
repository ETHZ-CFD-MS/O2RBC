FoamFile
{
    version     2.0;
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    symmetryA
    {
        type            symmetry;
        inGroups        1(symmetry);
        nFaces          0;
        startFace       0;
    }
    symmetryB
    {
        type            symmetry;
        inGroups        1(symmetry);
        nFaces          0;
        startFace       0;
    }
    RBC_to_nonRBC
    {
        type            mappedWall;
        nFaces          0;
        startFace       0;
        sampleMode      nearestPatchFace;
        sampleRegion    nonRBC;
        samplePatch     nonRBC_to_RBC;
    }
)

