4
(
    frontWedge
    {
        type            wedge;
        inGroups        1(wedge);
        nFaces          0;
        startFace       0;
    }
    backWedge
    {
        type            wedge;
        inGroups        1(wedge);
        nFaces          0;
        startFace       0;
    }
    membrane
    {
        type            patch;
        nFaces          0;
        startFace       0;
    }
    axis
    {
        type            empty;
        inGroups        1(empty);
        nFaces          0;
        startFace       0;
    }
)


