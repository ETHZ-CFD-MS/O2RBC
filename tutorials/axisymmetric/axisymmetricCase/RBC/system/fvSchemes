FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/RBC";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

divSchemes
{
    default         none;
}

gradSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}


